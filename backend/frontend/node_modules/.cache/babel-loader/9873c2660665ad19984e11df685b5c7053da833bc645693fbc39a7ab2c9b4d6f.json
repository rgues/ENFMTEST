{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { GET_REQUESTS, GET_ASSIGNATIONS, GET_CATEGORIES, GET_USERS } from '../store/types';\nimport { format } from \"date-fns\";\nexport default {\n  name: 'AssignationComponent',\n  async created() {\n    this.categories = await this.$store.dispatch(GET_CATEGORIES);\n    this.users = await this.$store.dispatch(GET_USERS);\n    this.requests = await this.$store.dispatch(GET_REQUESTS);\n    this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\n  },\n  data() {\n    return {\n      categories: [],\n      users: [],\n      requests: [],\n      assignations: [],\n      currentUser: this.$store.state.user.loginData.user\n    };\n  },\n  methods: {\n    categoryName(id) {\n      return this.categories.filter(item => item.id === id).map(item => item ? item.name : '')[0];\n    },\n    userName(id) {\n      return this.users.filter(item => item.id === id).map(item => item ? item.firstname + ' ' + item.lastname : '')[0];\n    },\n    requestDescription(id) {\n      return this.requests.find(item => item.id === id);\n    },\n    answerRequest(item) {\n      this.$router.push(`/admin/dashboard/${item.id}`);\n    },\n    reassignRequest(item) {\n      this.$router.push(`/admin/dashboard/${item.id}`);\n    },\n    formatDate(date) {\n      return format(date, \"yyyy-MM-dd\");\n    }\n  }\n};","map":{"version":3,"names":["GET_REQUESTS","GET_ASSIGNATIONS","GET_CATEGORIES","GET_USERS","format","name","created","categories","$store","dispatch","users","requests","assignations","data","currentUser","state","user","loginData","methods","categoryName","id","filter","item","map","userName","firstname","lastname","requestDescription","find","answerRequest","$router","push","reassignRequest","formatDate","date"],"sources":["C:\\Users\\HP\\Desktop\\Etisalat\\client\\src\\modules\\Assignation.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-container mt-5\">\r\n    <div className=\"admin_table\">\r\n  