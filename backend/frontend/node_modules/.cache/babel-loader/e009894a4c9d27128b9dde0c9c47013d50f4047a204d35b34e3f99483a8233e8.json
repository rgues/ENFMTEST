{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport AuthComponent from \"../layouts/Auth.vue\";\nimport { LOGIN_USER } from '../store/types';\nexport default {\n  name: \"HomeComponent\",\n  components: {\n    \"Auth-Layout\": AuthComponent\n  },\n  data() {\n    return {\n      email: '',\n      password: '',\n      loginError: '',\n      loading: false\n    };\n  },\n  methods: {\n    login(e) {\n      e.preventDefault();\n      this.loading = true;\n      const data = {\n        email: this.email,\n        password: this.password\n      };\n      this.$store.dispatch(LOGIN_USER, data).then(data => {\n        this.loading = false;\n        if (data === 'success') {\n          this.$router.push('/admin/dashboard');\n        }\n      }).catch(data => {\n        this.loading = false;\n        if (data === 'error') {\n          this.loginError = this.$store.state.user.message;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["AuthComponent","LOGIN_USER","name","components","data","email","password","loginError","loading","methods","login","e","preventDefault","$store","dispatch","then","$router","push","catch","state","user","message"],"sources":["C:\\Users\\HP\\Desktop\\Etisalat\\client\\src\\modules\\Home.vue"],"sourcesContent":["\r\n<template>\r\n  <Auth-Layout>\r\n    <div class=\"login-container d-sm-block p-5 mt-5 mt-sm-2\">\r\n\r\n      <form>\r\n  \r\n      <div class=\"mb-4\">\r\n        <h3 class=\"text-light\">AUTHENTICATION</h3>\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <p class=\"alert alert-danger\" v-show=\"loginError\" >{{loginError}}</p>\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <input\r\n         type=\"email\"\r\n          v-model=\"email\"\r\n          class=\"form-control\"\r\n          id=\"emailInput\"\r\n          placeholder=\"xyz@test.com\"\r\n        />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <input\r\n          type=\"password\"\r\n          v-model=\"password\"\r\n          class=\"form-control\"\r\n          id=\"passwordInput\"\r\n          placeholder=\"Password\"\r\n        />\r\n      </div>\r\n\r\n      <div class=\"mb-3\">\r\n        <button\r\n         :disabled=\"loading\"\r\n          @click=\"login\"\r\n          class=\"btn btn-danger\"\r\n          type=\"button\"\r\n        >\r\n          Login\r\n        </button>\r\n      </div>\r\n       </form>\r\n    </div>\r\n  </Auth-Layout>\r\n</template>\r\n\r\n<script>\r\nimport AuthComponent from \"../layouts/Auth.vue\";\r\nimport {LOGIN_USER} from '../store/types';\r\n\r\nexport default {\r\n  name: \"HomeComponent\",\r\n  \r\n  components: {\r\n    \"Auth-Layout\": AuthComponent,\r\n  },\r\n\r\n  data ()  { \r\n   return {\r\n      email: '',\r\n      password:'',\r\n      loginError: '',\r\n      loading:false\r\n  }\r\n}, \r\n  methods: {\r\n      login(e) {\r\n        e.preventDefault();\r\n          this.loading = true;\r\n         const data = {\r\n           email : this.email,\r\n           password : this.password\r\n         }\r\n         this.$store.dispatch(LOGIN_USER,data).then(data => {\r\n              this.loading = false;\r\n              if(data === 'success') {\r\n                this.$router.push('/admin/dashboard');\r\n              }\r\n         }).catch(data => {\r\n            this.loading = false;\r\n           if(data === 'error') {\r\n              this.loginError = this.$store.state.user.message;\r\n            } \r\n         })\r\n         \r\n      }\r\n  }\r\n\r\n};\r\n\r\n</script>\r\n\r\n<style>\r\n.login-container {\r\n  border-radius: 1rem;\r\n  border: solid 1px #222;\r\n  width: 40%;\r\n  margin: auto;\r\n  background: var(--bs-dark);\r\n  color: #fff;\r\n}\r\n\r\n@media (max-width: 645px) {\r\n  .login-container {\r\n    width: 100%;\r\n  }\r\n}\r\n</style>"],"mappings":";AAkDA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,SAAQC,UAAU,QAAO,gBAAgB;AAEzC,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,UAAU,EAAE;IACV,aAAa,EAAEH;EACjB,CAAC;EAEDI,IA