\">\r\n        <button\r\n          @click=\"login\"\r\n          class=\"btn btn-danger\"\r\n          type=\"button\"\r\n        >\r\n          Login\r\n        </button>\r\n      </div>\r\n       </form>\r\n    </div>\r\n  </Auth-Layout>\r\n</template>\r\n\r\n<script>\r\nimport AuthComponent from \"../layouts/Auth.vue\";\r\nimport {LOGIN_USER} from '../store/types';\r\nimport { onUpdated } from 'vue';\r\nimport { router } from 'vue-router';\r\n\r\nexport default {\r\n  name: \"HomeComponent\",\r\n  \r\n  components: {\r\n    \"Auth-Layout\": AuthComponent,\r\n  },\r\n\r\n  data ()  { \r\n   return {\r\n      email: '',\r\n      password:'',\r\n      loginError:''\r\n  }\r\n}, \r\n  methods: {\r\n      login(e) {\r\n        e.preventDefault();\r\n         const data = {\r\n           email : this.email,\r\n           password : this.password\r\n         }\r\n         this.$store.dispatch(LOGIN_USER,data)\r\n      }\r\n  }\r\n\r\n};\r\n\r\n  onUpdated(() => {\r\n   \r\n      if (this.$store.state.user.isAuthenticated) {\r\n          router.push('/admin/dashboard');\r\n      }\r\n })\r\n\r\n</script>\r\n\r\n<style>\r\n.login-container {\r\n  border-radius: 1rem;\r\n  border: solid 1px #222;\r\n  width: 40%;\r\n  margin: auto;\r\n  background: var(--bs-dark);\r\n  color: #fff;\r\n}\r\n\r\n@media (max-width: 645px) {\r\n  .login-container {\r\n    width: 100%;\r\n  }\r\n}\r\n</style>"],"mappings":";AAiDA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,SAAQC,UAAU,QAAO,gBAAgB;AACzC,SAASC,SAAQ,QAAS,KAAK;AAC/B,SAASC,MAAK,QAAS,YAAY;AAEnC,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,UAAU,EAAE;IACV,aAAa,EAAEL;EACjB,CAAC;EAEDM,IAAGA,CAAA,EAAM;IACR,OAAO;MACJC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAC,EAAE;MACXC,UAAU,EAAC;IACf;EACF,CAAC;EACCC,OAAO,EAAE;IACLC,KAAKA,CAACC,CAAC,EAAE;MACPA,CAAC,CAACC,cAAc,CAAC,CAAC;MACjB,MAAMP,IAAG,GAAI;QACXC,KAAI,EAAI,IAAI,CAACA,KAAK;QAClBC,QAAO,EAAI,IAAI,CAACA;MAClB;MACA,IAAI,CAACM,MAAM,CAACC,QAAQ,CAACd,UAAU,EAACK,IAAI;IACvC;EACJ;AAEF,CAAC;AAECJ,SAAS,CAAC,MAAM;EAEZ,IAAI,IAAI,CAACY,MAAM,C