{"ast":null,"code":"import { GET_REQUESTS, GET_ASSIGNATIONS, GET_CATEGORIES, GET_ANSWERS, APPROVE_ANSWER, GET_USERS } from '../store/types';\nimport { format } from \"date-fns\";\nexport default {\n  name: 'AnswerComponent',\n  async created() {\n    this.categories = await this.$store.dispatch(GET_CATEGORIES);\n    this.users = await this.$store.dispatch(GET_USERS);\n    this.requests = await this.$store.dispatch(GET_REQUESTS);\n    this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\n    this.answers = await this.$store.dispatch(GET_ANSWERS);\n  },\n  data() {\n    return {\n      categories: [],\n      users: [],\n      requests: [],\n      assignations: [],\n      answers: [],\n      currentUser: this.$store.state.user.loginData.user\n    };\n  },\n  methods: {\n    categoryName(id) {\n      return this.categories.filter(item => item.id === id).map(item => item ? item.name : '')[0];\n    },\n    userName(id) {\n      return this.users.filter(item => item.id === id).map(item => item ? item.firstname + ' ' + item.lastname : '')[0];\n    },\n    requestDescription(id) {\n      return this.requests.find(item => item.id === id);\n    },\n    assignationComment(id) {\n      return this.assignations.find(item => item.id === id);\n    },\n    approveResponse(item) {\n      console.log('reassign a request to other user');\n      console.log(item);\n      const data = {\n        description: '',\n        assignation_id: '',\n        state: \"approve\",\n        id: item.id\n      };\n      this.$store.dispatch(APPROVE_ANSWER, data).then(async reponse => {\n        if (reponse.status === 'success') {\n          this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\n        } else {\n          alert(this.$store.request.message);\n        }\n      });\n    },\n    formatDate(date) {\n      return format(date, \"yyyy-MM-dd\");\n    }\n  }\n};","map":{"version":3,"names":["GET_REQUESTS","GET_ASSIGNATIONS","GET_CATEGORIES","GET_ANSWERS","APPROVE_ANSWER","GET_USERS","format","name","created","categories","$store","dispatch","users","requests","assignations","answers","data","currentUser","state","user","loginData","methods","categoryName","id","filter","item","map","userName","firstname","lastname","requestDescription","find","assignationComment","approveResponse","console","log","description","assignation_id","then","reponse","status","alert","request","message","formatDate","date"],"sources":["C:\\Users\\HP\\Desktop\\Etisalat\\client\\src\\modules\\Answer.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-container mt-5\">\r\n    <div className=\"admin_table\">\r\n      <table class=\"table\" >\r\n        <thead>\r\n          <tr class=\"table-secondary\">\r\n            <th scope=\"col\">Ticket</th>\r\n            <th scope=\"col\">Category</th>\r\n            <th scope=\"col\">Description</th>\r\n            <th scope=\"col\">Status</th>\r\n            <th scope=\"col\">Comment</th>\r\n            <th scope=\"col\">IT</th>\r\n            <th scope=\"col\">Response</th>\r\n            <th scope=\"col\">Date</th>\r\n            <th scope=\"col\">Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n           <tr class=\"table-light\" v-for=\"item in assignations\" :key=\"item.id\" >\r\n\r\n                    <td>\r\n                      {{requestDescription(item.request_id) ? requestDescription(item.request_id).title : ''}}\r\n                    </td>\r\n                    <td>\r\n                        <span class=\"fw-bold\" >\r\n                            {{requestDescription(item.request_id) ? categoryName(requestDescription(item.request_id).category_id) : '' }}\r\n                        </span>\r\n                    </td>\r\n                   \r\n                    <td>\r\n                            {{requestDescription(item.request_id) ? requestDescription(item.request_id).description : ''}}\r\n                    </td>\r\n                    <td>\r\n                      <span :class=\"{'badge bg-danger':item.state==='pending', 'badge bg-warning':item.state==='evaluate', 'badge bg-success':item.state==='complete' }\">\r\n                        {{item.state}}\r\n                      </span> \r\n                      </td>\r\n                    <td>\r\n                        <span class=\"fs-6\" >\r\n                            {{item.comment}}\r\n                        </span>\r\n                    </td>\r\n                    <td>{{userName(item.user_id) }}</td>\r\n                  \r\n                    <td>{{formatDate(item.date_assignation)}}</td>\r\n                    <td>\r\n                     \r\n                        <button @click=\"approveResponse(item)\" :disabled=\"item.state==='approve'\" v-if=\"currentUser.role_id ===2\" :class=\"{'btn btn-small btn-secondary':item.state==='in_approval' , \r\n                        'btn btn-small btn-light':item.state==='complete' }\" >\r\n                            Confirm\r\n                        </button>\r\n                    </td>\r\n               </tr> \r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { GET_REQUESTS,GET_ASSIGNATIONS, GET_CATEGORIES,GET_ANSWERS, APPROVE_ANSWER,GET_USERS} from '../store/types';\r\nimport { format } from \"date-fns\";\r\n\r\nexport default { \r\n  \r\n    name:'AnswerComponent',\r\n    async created() { \r\n       this.categories = await this.$store.dispatch(GET_CATEGORIES);\r\n       this.users = await this.$store.dispatch(GET_USERS);\r\n       this.requests = await this.$store.dispatch(GET_REQUESTS);\r\n       this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\r\n       this.answers = await this.$store.dispatch(GET_ANSWERS);\r\n    },\r\n    data() {\r\n        return {  \r\n            categories : [],\r\n            users: [],\r\n            requests : [],\r\n            assignations:[],\r\n            answers:[],\r\n            currentUser: this.$store.state.user.loginData.user\r\n        }\r\n    },\r\n    methods : {\r\n        categoryName(id) {\r\n           return this.categories.filter(item => item.id === id).map(item=>item ? item.name : '')[0];\r\n        },\r\n         userName(id) {\r\n           return this.users.filter(item => item.id === id).map(item=>item ? item.firstname + ' ' + item.lastname : '')[0];\r\n        }\r\n        ,\r\n         requestDescription(id) {\r\n           return this.requests.find(item => item.id === id);\r\n        }\r\n        ,\r\n         assignationComment(id) {\r\n           return this.assignations.find(item => item.id === id);\r\n        }\r\n       ,\r\n        approveResponse(item) {\r\n            console.log('reassign a request to other user')\r\n            console.log(item)\r\n            const data = { \r\n                description:'',\r\n                assignation_id:'',\r\n                state:\"approve\",\r\n                id: item.id\r\n            }\r\n            this.$store.dispatch(APPROVE_ANSWER,data).then(async reponse => {\r\n                if (reponse.status === 'success') {\r\n                    this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\r\n                } else {\r\n                    alert(this.$store.request.message);\r\n                }\r\n            });\r\n        },\r\n        formatDate(date) {\r\n                return format(date,\"yyyy-MM-dd\");\r\n        }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA6DA,SAASA,YAAY,EAACC,gBAAgB,EAAEC,cAAc,EAACC,WAAW,EAAEC,cAAc,EAACC,SAAS,QAAO,gBAAgB;AACnH,SAASC,MAAK,QAAS,UAAU;AAEjC,eAAe;EAEXC,IAAI,EAAC,iBAAiB;EACtB,MAAMC,OAAOA,CAAA,EAAG;IACb,IAAI,CAACC,UAAS,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAACT,cAAc,CAAC;IAC5D,IAAI,CAACU,KAAI,GAAI,MAAM,IAAI,CAACF,MAAM,CAACC,QAAQ,CAACN,SAAS,CAAC;IAClD,IAAI,CAACQ,QAAO,GAAI,MAAM,IAAI,CAACH,MAAM,CAACC,QAAQ,CAACX,YAAY,CAAC;IACxD,IAAI,CAACc,YAAW,GAAI,MAAM,IAAI,CAACJ,MAAM,CAACC,QAAQ,CAACV,gBAAgB,CAAC;IAChE