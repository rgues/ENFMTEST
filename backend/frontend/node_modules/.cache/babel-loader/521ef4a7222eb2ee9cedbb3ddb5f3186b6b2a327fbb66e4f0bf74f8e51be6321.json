{"ast":null,"code":"import { GET_REQUESTS, GET_ASSIGNATIONS, GET_CATEGORIES, GET_USERS } from '../store/types';\nexport default {\n  name: 'AssignationComponent',\n  async created() {\n    this.categories = await this.$store.dispatch(GET_CATEGORIES);\n    this.users = await this.$store.dispatch(GET_USERS);\n    this.requests = await this.$store.dispatch(GET_REQUESTS);\n    this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\n  },\n  data() {\n    return {\n      categories: [],\n      users: [],\n      requests: [],\n      assignations: [],\n      currentUser: this.$$store.state.user.loginData.user\n    };\n  },\n  methods: {\n    categoryName(id) {\n      return this.categories.filter(item => item.id === id).map(item => item ? item.name : '')[0];\n    },\n    userName(id) {\n      return this.users.filter(item => item.id === id).map(item => item ? item.firstname + ' ' + item.lastname : '')[0];\n    },\n    requestDescription(id) {\n      return this.requests.find(item => item.id === id);\n    },\n    answerRequest() {\n      console.log('answer request');\n    }\n  }\n};","map":{"version":3,"names":["GET_REQUESTS","GET_ASSIGNATIONS","GET_CATEGORIES","GET_USERS","name","created","categories","$store","dispatch","users","requests","assignations","data","currentUser","$$store","state","user","loginData","methods","categoryName","id","filter","item","map","userName","firstname","lastname","requestDescription","find","answerRequest","console","log"],"sources":["C:\\Users\\HP\\Desktop\\Etisalat\\client\\src\\modules\\Assignation.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-container mt-5\">\r\n    <div className=\"admin_table\">\r\n      <table class=\"table\" >\r\n        <thead>\r\n          <tr class=\"table-secondary\">\r\n            <th scope=\"col\">Ticket</th>\r\n            <th scope=\"col\">Category</th>\r\n            <th scope=\"col\">Description</th>\r\n            <th scope=\"col\">Status</th>\r\n             <th scope=\"col\">Comment</th>\r\n            <th scope=\"col\">Sender</th>\r\n            <th scope=\"col\">Date</th>\r\n            <th scope=\"col\">Action</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n           <tr class=\"table-light\" v-for=\"item in assignations\" :key=\"item.id\" >\r\n                    <td>\r\n                        <span class=\"fw-bold\" >\r\n                            {{categoryName(item.category_id) }}\r\n                        </span>\r\n                    </td>\r\n                    <td>\r\n                      <span class=\"fw-normal\" >{{requestDescription(item.request_id) ? requestDescription(item.request_id).title : ''}}</span>\r\n                    </td>\r\n                    <td>\r\n                        <span class=\"fs-4\">\r\n                            {{requestDescription(item.request_id) ? requestDescription(item.request_id).description : ''}}\r\n                        </span>\r\n                    </td>\r\n                    <td>\r\n                      <span :class=\"{'badge bg-danger':item.state==='pending', 'badge bg-warning':item.state==='evaluate', 'badge bg-success':item.state==='complete' }\">\r\n                        {{item.state}}\r\n                      </span> \r\n                      </td>\r\n                    <td>\r\n                        <span class=\"fs-6\" >\r\n                            {{item.Comment}}\r\n                        </span>\r\n                    </td>\r\n                    <td>{{userName(item.sender_id) }}</td>\r\n                  \r\n                    <td>{{item.date_assignation}}</td>\r\n                    <td>\r\n                        <button @click=\"answerRequest\" :disabled=\"item.state==='complete'\" class=\"btn btn-small btn-primary\"  v-show=\"currentUser.role_id===3\">\r\n                            Answer\r\n                        </button>\r\n                        <button @click=\"answerRequest\" :disabled=\"item.state==='complete'\" class=\"btn btn-small btn-danger\"  v-show=\"currentUser.role_id!==2\">\r\n                            Reassign\r\n                        </button>\r\n                    </td>\r\n               </tr> \r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { GET_REQUESTS,GET_ASSIGNATIONS, GET_CATEGORIES, GET_USERS} from '../store/types';\r\n\r\nexport default { \r\n  \r\n    name:'AssignationComponent',\r\n    async created() { \r\n       this.categories = await this.$store.dispatch(GET_CATEGORIES);\r\n       this.users = await this.$store.dispatch(GET_USERS);\r\n       this.requests = await this.$store.dispatch(GET_REQUESTS);\r\n       this.assignations = await this.$store.dispatch(GET_ASSIGNATIONS);\r\n    },\r\n    data() {\r\n        return {  \r\n            categories : [],\r\n            users: [],\r\n            requests : [],\r\n            assignations:[],\r\n            currentUser: this.$$store.state.user.loginData.user\r\n        }\r\n    },\r\n    methods : {\r\n        categoryName(id) {\r\n           return this.categories.filter(item => item.id === id).map(item=>item ? item.name : '')[0];\r\n        },\r\n         userName(id) {\r\n           return this.users.filter(item => item.id === id).map(item=>item ? item.firstname + ' ' + item.lastname : '')[0];\r\n        }\r\n        ,\r\n         requestDescription(id) {\r\n           return this.requests.find(item => item.id === id);\r\n        }\r\n        ,answerRequest() {\r\n            console.log('answer request')\r\n        }\r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n</style>"],"mappings":"AA6DA,SAASA,YAAY,EAACC,gBAAgB,EAAEC,cAAc,EAAEC,SAAS,QAAO,gBAAgB;AAExF,eAAe;EAEXC,IAAI,EAAC,sBAAsB;EAC3B,MAAMC,OAAOA,CAAA,EAAG;IACb,IAAI,CAACC,UAAS,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,QAAQ,CAACN,cAAc,CAAC;IAC5D,IAAI,CAACO,KAAI,GAAI,MAAM,IAAI,CAACF,MAAM,CAACC,QAAQ,CAACL,SAAS,CAAC;IAClD,IAAI,CAACO,QAAO,GAAI,MAAM,IAAI,CAACH,MAAM,CAACC,QAAQ,CAACR,YAAY,CAAC;IACxD,IAAI,C